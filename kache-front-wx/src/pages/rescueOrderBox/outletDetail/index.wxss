/* src/pages/rescueOrderBox/outletDetail/index.wxss */

.images_box {
  width: 100%;
  height: 240px;
  position: relative;
  background-color: #fff;
}

.images_box image {
  width: 100%;
  height: 100%;
}

.store_address {
  position: absolute;
  bottom: 0;
  width: 100%;
  line-height: 60rpx;
  background-color: rgba(0, 0, 0, 0.6);
  color: #fff;
  font-size: 14px;
}

.store_address image {
  width: 20px;
  height: 20px;
  vertical-align: middle;
}

.msg_box {
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  background-color: #fff;
}

.msg_box .store_name {
  text-indent: 10rpx;
  font-size: 30rpx;
  font-weight: bold;
  color: #000;
  float: left;
}

.msg_box .store_mobile {
  float: right;
  width: 100rpx;
  height: 100rpx;
  background-image: url(data:image/png;base64,);
  background-size: 50rpx;
  background-position: center;
  background-repeat: no-repeat;
  background-color: #4d94f5;
}

.serviceItems_box {
  background-color: #fff;
  border-top: 20rpx solid #888;
}

.serviceItems_name {
  height: 100rpx;
  line-height: 100rpx;
  font-size: 16px;
  text-indent: 10rpx;
}

.table {
  border: 1px solid #dadada;
  border-right: 0;
  border-bottom: 0;
  width: 98%;
  margin-left: 1%;
  margin-bottom: 20rpx;
}

.tr {
  width: 100%;
  display: flex;
  justify-content: space-between;
}

.trhover {
  background: #4d94f5;
}

.trhover .td {
  color: #fff;
}

.th, .td {
  height: 56rpx;
  line-height: 56rpx;
  border-bottom: 1px solid #dadada;
  border-right: 1px solid #dadada;
  text-align: center;
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.th {
  font-weight: 800;
  background-color: #4d94f5;
  font-size: 22rpx;
  color: #fff;
}

.td {
  font-size: 22rpx;
  color: #303030;
}

.btn {
  margin-bottom: 70rpx;
}
